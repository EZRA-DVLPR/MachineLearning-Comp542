import nbformat

def merge_notebooks(notebook_paths, output_path):
    merged_notebook = nbformat.v4.new_notebook()

    for notebook_path in notebook_paths:
        with open(notebook_path, 'r', encoding='utf-8') as notebook_file:
            notebook_content = nbformat.read(notebook_file, as_version=4)
            merged_notebook.cells.extend(notebook_content.cells)

    with open(output_path, 'w', encoding='utf-8') as output_file:
        nbformat.write(merged_notebook, output_file)

if __name__ == "__main__":
    notebook_paths = ["C:\\Users\\derpi\\Documents\\GitHub\\Comp_542_proj\\group_proj\\JT.ipynb", "C:\\Users\\derpi\\Documents\\GitHub\\Comp_542_proj\\group_proj\\project_copy.ipynb"]
    output_path = "combined_notebook.ipynb"
    merge_notebooks(notebook_paths, output_path)
